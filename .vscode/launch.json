{
    "version": "0.2.0",
    "configurations": [  
        {
            "name": "(gdb) test (all ifj24 tests)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test/build_test/ifj24_compiler_test",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/src",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Skip Google Test functions",
                    "text": "skip pair_getters::*, internal::*, variant_matcher::*, any_cast_matcher::*, no_adl::*, gmock::*, gtest::*, testing::*",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "make: build-test"
        },
